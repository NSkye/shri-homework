{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": ["src/*"],
      "components/*": ["src/components/*"],
      "pages/*": ["src/pages/*"],
      "utils/*": ["src/utils/*"],
      "libs/*": ["src/libs/*"]
    },
    "target": "es2015",
    "strict": true,
    "module": "es2015",
    "moduleResolution": "node"
  }
}